# LSBATCH: User input
#!/bin/bash
# Batch script to test blk_offload with different block sizes & elements per thread
# Batch script to test all offload methods
#BSUB -J mm_batch_gpu
#BSUB -o mm_batch_gpu_%J.out
#BSUB -q hpcintrogpu
#BSUB -n 32
#BSUB -R "rusage[mem=2048]"
#BSUB -W 15
#BSUB -R "span[hosts=1]"
#BSUB -gpu "num=1:mode=exclusive_process"

METHOD="blk_offload"
SIZES="100 200 500 1000 2000 5000"
BLOCK_SIZES="32 64 128"
ELEMENTS="1 2 4 8"

RESULTS_FILE="offload_performance_blocks_vs_elements.csv"

echo "Method, Size, Block Size, Elements Per Thread, Kernel (s), GFLOPS/s" > $RESULTS_FILE

module load nvhpc/24.11

for SIZE in $SIZES; do
    for BLOCK in $BLOCK_SIZES; do
        for ELEMENT in $ELEMENTS; do
            echo "Testing $METHOD with size $SIZE, block size $BLOCK, elements per thread $ELEMENT..."

            # Run executable and capture output
            OUTPUT=$(./matmult_c.nvc++ "$METHOD" "$SIZE" "$SIZE" "$SIZE" "$BLOCK" "$ELEMENT" 2>&1 | tee /dev/tty)

            # Extract Kernel time **correctly**
            RESULT_LINE=$(echo "$OUTPUT" | grep -E "^blk_offload,")
            
            # Debug: Print the matched line
            echo "Extracted Line: $RESULT_LINE"

            KERNEL=$(echo "$RESULT_LINE" | awk -F', ' '{print $3}')
            KERNEL=${KERNEL:-0}  # Ensure it defaults to 0 if empty

            # Compute GFLOPS/s
            if [[ "$KERNEL" == "0" || "$KERNEL" == "" ]]; then
                GFLOPS="0"
            else
                GFLOPS=$(echo "scale=6; (2 * $SIZE * $SIZE * $SIZE) / ($KERNEL * 1.0e9)" | bc)
            fi

            # Log results
            echo "$METHOD, $SIZE, $BLOCK, $ELEMENT, $KERNEL, $GFLOPS" >> $RESULTS_FILE
            echo "Saved: $METHOD, $SIZE, $BLOCK, $ELEMENT, $KERNEL, $GFLOPS"

