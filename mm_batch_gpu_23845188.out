            fi

            # Log results
            echo "$METHOD, $SIZE, $ELEMENT, $KERNEL, $GFLOPS" >> $RESULTS_FILE
            echo "Saved: $METHOD, $SIZE, $ELEMENT, $KERNEL, $GFLOPS"
        done
